    <property name="styleSheet">
     <string notr="true">QSlider::groove:horizontal {
    border: none;                     /* Remove the groove border */
    height: 6px;                      /* Groove height */
    background: #4fa3d9;              /* Groove color (blue-gray) */
    border-radius: 3px;               /* Rounded groove edges */
    margin: 0px;                      /* Align properly */
}

QSlider::handle:horizontal {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 1,
        stop: 0 #70d6ff,               /* Light blue at start */
        stop: 1 #0077cc                /* Darker blue at end */
    );
    border: none;                     /* No border for handle */
    width: 12px;                      /* Handle width */
    height: 12px;                     /* Handle height */
    border-radius: 6px;               /* Circular handle */
    margin: -3px 0;                   /* Align handle perfectly with groove */
}

QSlider::sub-page:horizontal {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 1,
        stop: 0 #70d6ff,               /* Gradient starts light blue */
        stop: 1 #0077cc                /* Gradient ends darker blue */
    );
    border: none;                     /* No border for filled portion */
    height: 6px;                      /* Same as groove height */
    border-radius: 3px;               /* Rounded edges */
}

QSlider::add-page:horizontal {
    background: #aaaaaa;              /* Unfilled portion color (light gray) */
    border: none;                     /* No border for unfilled portion */
    height: 6px;                      /* Same as groove height */
    border-radius: 3px;               /* Rounded edges */
}
</string>
